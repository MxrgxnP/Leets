#include<stdio.h>
int main(void) {
    int mark;
    printf("Insert me your vote here\n");
    scanf("%d", &mark);
    if (mark>0 && mark<30) {
        if (mark<10) {
            printf("Gravemente insufficiente, capra\n");
        } else if (mark>=10 && mark<=17) {
            printf("Insufficiente\n");
        } else if (mark>=18 && mark<=24) {
            printf("Sufficiente\n");
        } else if (mark>24) {
            printf("Ottimo\n");
        }
    } else {
        printf("Error");
    }
}
