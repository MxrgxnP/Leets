#include<stdio.h>
#define M 3
#define N 4
int main(void){
  int n, i, j;
  int m1[M][N];
  int proprieta = 0;
  int start = 0;

    // Acquisizione dati
  scanf("%d", &n);
  while(n<=0)
    scanf("%d", &n);

  for(i=0;i<M;i++){
    for(j=0;j<N;j++){
      scanf("%d",&m1[i][j]);
    }
  }

  while (n < N && n < N) {
    for(i = start;i <= n; i ++){
      for(j = start; j <= n; j ++){
        if (m1[i][j] == 0) {
          proprieta = 1;
        }

      }
    }
    start = start + n;
    n = n + n;
  }

  printf("%d",proprieta);
}
