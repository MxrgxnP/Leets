#include<stdio.h>
#define DIM 10

int main(void){
  int array[DIM];
  int i, inizio = 0, fine = 0; //numeroCifre = 0;
  int conte = 1;
  int flag = 0;

  for (i = 0; i < DIM; i++) {
    scanf("%d", &array[i]);
  }

  for (i = 0; i < DIM; i++) {
    if (array[i] == array[i+1]) {
      inizio = i - (conte - 1);
      conte ++;
      flag = 1;
    } else {
        fine = inizio + (conte - 1);
 //     numeroCifre = conte;
 //     conte = 0;
    }
  }

  if (flag == 1) {
    printf("%d %d %d", conte, inizio, fine);
  } else {
    for (i = 0; i < DIM; i ++) {
      printf("%d", array[i]);
    }
  }

  return 0;
}
