#include<stdio.h>
#define D 8

int main(void){
  int A[D];
  int i, lastFigure;

  for (i = 0; i < D; i++) {
    scanf("%d", &A[i]);
  }

  for (i = 0; i < D; i++) {
    lastFigure = A[i] % 100;
    if (lastFigure <= 9) {
      A[i] = lastFigure;
    } else {
      A[i] = A[i] / 10;
    }
  }

  for (i = 0; i < D; i++) {
    if (i < D-1) {
      printf("%d,", A[i]);
    } else {
      printf("%d", A[i]);
    }
  }

    return 0;
}
